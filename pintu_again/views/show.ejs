    <div class="cards flex flex-wrap gap-10 px-6 pt-2 max-lg:items-center max-lg:justify-center">
        <% user.posts.map((post)=>{%>
        
        <div class="w-32">
            <div class=" w-full rounded-lg min-h-40 overflow-hidden">
